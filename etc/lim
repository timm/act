#!/usr/bin/env ./gold
# vim: filetype=awk ts=2 sw=2 sts=2 et :

BEGIN { num(i) ; add(i,20) }

function has(i,k)    { i.k[0]; delete i.k }
function add(i,x, f) { f=i.is"Add"; return @f(i,x) }

function obj(i)      { split("",i,"") }
function col(i,c,s)  { obj(i); i.n=0; i.at=c; i.is="col"; i.txt=s; i.w= s~/-/?-1:1 }
function num(i,c,s)  { col(i,c,s);    i.is="num"; i.n=i.mu=i.m2=0; has(i,"has") }
function numAdd(i,x) { print(10*x) }
